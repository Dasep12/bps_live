                                MinOrder = 1;
                                MaxOrder = 0;
                            }

                            //$("#crud-ReturnPartDetailOnOrderQty").val(OnOrder);
                            $("#crud-ReturnPartDetailReturnUnitQty").val(MaxOrder).attr("min", MinOrder).attr("max", MaxOrder).focus().select();

                            break;
                        case "Update":

                            crudReturnPartDetail(action, searchresult.PartNumber);
                            $("#crud-ReturnPartDetailReturnUnitQty").focus().select();

                            break;
                    }
                }
            }, 100);
