                            $("#crud-ReturnPartDetailPackingId").val(searchresult.PackingId);

                            var OnOrder = GetOnOrderStock($("#crud-ReturnPartNumber").val(), searchresult.SupplierId, searchresult.PartNumber);
                            if (OnOrder === null || OnOrder === "") {
                                OnOrder = 0;
