</div>

<div class="modal animated fadeIn" id="crudReturnPartDetailModal" tabindex="-1" role="dialog" aria-hidden="true" data-keyboard="false" data-backdrop="false">
    <div class="modal-dialog modal-lg modal-dialog-centered" role="document">
        <div class="modal-content">
            <form id="crudReturnPartDetailForm" class="needs-validation" novalidate>
                <div class="modal-header">
                    <h6 class="modal-title"><span class="fa fa-plus-square"></span> ReturnPartDetail</h6>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <div class="modal-body">
                    <a href="#" id="btn-searchPart" class="btn btn-sm btn-outline-primary" onclick="callFilterStockRawReturn()"><span class="fa fa-search"></span> Find Part Number</a>
                    <hr />
                    <div class="row">
                        <div class="col-md-6">
                            <div class="form-group form-group-sm row">
                                <label for="crud-ReturnPartDetailUniqueNumber" class="col-md-4 col-form-label col-form-label-sm">Unique Number</label>
                                <div class="col-md-8">
                                    <input type="text" class="form-control form-control-sm bg-light" id="crud-ReturnPartDetailUniqueNumber" required readonly>
                                </div>
                            </div>
                            <div class="form-group form-group-sm row">
                                <label for="crud-ReturnPartDetailPartNumber" class="col-md-4 col-form-label col-form-label-sm">Part Number</label>
                                <div class="col-md-8">
                                    <input type="text" class="form-control form-control-sm bg-light" id="crud-ReturnPartDetailPartNumber" readonly>
                                </div>
                            </div>
                            <div class="form-group form-group-sm row">
                                <label for="crud-ReturnPartDetailPartName" class="col-md-4 col-form-label col-form-label-sm">Part Name</label>
                                <div class="col-md-8">
                                    <input type="text" class="form-control form-control-sm bg-light" id="crud-ReturnPartDetailPartName" readonly>
                                </div>
                            </div>
                            <div class="form-group form-group-sm row">
                                <label for="crud-ReturnPartDetailPartModel" class="col-md-4 col-form-label col-form-label-sm">Model</label>
                                <div class="col-md-8">
                                    <input type="text" class="form-control form-control-sm bg-light" id="crud-ReturnPartDetailPartModel" readonly>
                                </div>
                            </div>
                            <div class="form-group form-group-sm row">
                                <label for="crud-ReturnPartDetailPackingId" class="col-md-4 col-form-label col-form-label-sm">Type Packing</label>
                                <div class="col-md-8">
                                    <input type="text" class="form-control form-control-sm bg-light" id="crud-ReturnPartDetailPackingId" readonly>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class="form-group form-group-sm row">
                                <label for="crud-ReturnPartDetailUnit" class="col-md-4 col-form-label col-form-label-sm">Unit</label>
                                <div class="col-md-8">
                                    <input type="text" class="form-control form-control-sm bg-light" id="crud-ReturnPartDetailUnit" readonly>
                                </div>
                            </div>
                            <div class="form-group form-group-sm row">
                                <label for="crud-ReturnPartDetailStockQty" class="col-md-4 col-form-label col-form-label-sm">NG Stock</label>
                                <div class="col-md-4">
                                    <div class="input-group input-group-sm" title="NG Stock Qty">
                                        <div class="input-group-prepend">
                                            <span class="input-group-text">QTY</span>
                                        </div>
                                        <input type="text" class="form-control form-control-sm" id="crud-ReturnPartDetailStockQty" required>
                                    </div>
                                </div>                                
                            </div>                            
                            <div class="form-group form-group-sm row">
                                <label for="crud-ReturnPartDetailReturnUnitQty" class="col-md-4 col-form-label col-form-label-sm">Return Unit Qty</label>
                                <div class="col-md-4">
                                    <input type="number" min="1" class="form-control form-control-sm" id="crud-ReturnPartDetailReturnUnitQty" required>
                                </div>
                            </div>
                        </div>
                    </div>

                    <input type="hidden" id="ReturnPartDetailAction" />
                    <input type="hidden" id="ReturnPartDetailRowStatus" />
                    <div id="crudReturnPartDetailError"></div>

                </div>
                <div class="modal-footer">
                    <div class="ml-3" style="position:absolute;left:0">
                        <a href="#" id="btn-prev" class="btn btn-sm btn-outline-primary" onclick="crudReturnPartDetailMove('prev')"><span class="fa fa-arrow-left fa-2x"></span></a>
                        <a href="#" id="btn-next" class="btn btn-sm btn-outline-primary" onclick="crudReturnPartDetailMove('next')"><span class="fa fa-arrow-right fa-2x"></span></a>
                    </div>
                    <button id="btn-crudReturnPartDetail" type="submit" class="btn btn-sm btn-primary btn-spin" dismiss="modal"><span class="fa fa-dot-circle-o"></span> Submit</button>
                    <button type="button" class="btn btn-sm btn-danger" data-dismiss="modal"><span class="fa fa-times"></span> Cancel</button>
                </div>
            </form>

        </div>
    </div>
</div>

<script>

    var canSelect = false;

    $('#crudReturnPartDetailModal').on('keydown', 'input, select', function (e) {
        if (e.which === 13) {
            var self = $(this), form = self.parents('form:eq(0)'), focusable, next;
            focusable = form.find('input').filter(':visible');
            next = focusable.eq(focusable.index(this) + 1);
            if (next.length) {
                next.focus();
            }
            if (this.id === "crud-ReturnPartDetailReturnUnitQty") {
                $("#btn-crudReturnPartDetail").focus();
            };
            return false;
        }
    });

    function crudReturnPart(action, id) {

        //call partial on _Layout
        loadblockspinner();
        $('#search_content').html("").load('@Url.Action("SearchPartStockRawReturn","Search")');

        document.getElementById("crudReturnPartForm").reset();
        $('#crudReturnPartForm').removeClass('was-validated');
        $('#crudReturnPartError').html("");
        $('#btn-addReturnPartDetail').removeAttr('disabled');
        $('#btn-scanReturnPartDetail').removeAttr('disabled');
        $("#btn-crudReturnPart").html("<span class='fa fa-dot-circle-o'></span> Submit");
        $("#crudReturnPartForm input,select").each(function () {
            $(this).removeAttr("readonly").removeAttr("disabled").removeClass('error').next('label.error').remove().val("");
        });

        $("#crud-showhide").removeAttr("checked");
        $("#approvalInfo").text("");
        $("#ReturnPartAction").val(action);

        var newdate = '@ViewBag.DateTime';
        newdate = moment(new Date(newdate)).format("YYYY-MM-DD");

        if (id != "*") {
            var Grid = $('#jqGridMain'),
                selectedRowId = id,
                ReturnPartStatus         = Grid.jqGrid('getCell', selectedRowId, 'Status').split('>'),
                ReturnPartDate           = Grid.jqGrid('getCell', selectedRowId, 'ReturnDate'),
                ReturnPartSupplierId     = Grid.jqGrid('getCell', selectedRowId, 'SupplierId'),
                ReturnPartIncomingDate   = Grid.jqGrid('getCell', selectedRowId, 'IncomingDate'),
                ReturnPartIncomingTime   = Grid.jqGrid('getCell', selectedRowId, 'IncomingTime'),
                ReturnPartApproval       = Grid.jqGrid('getCell', selectedRowId, 'Approval'),
                ReturnPartRemarks        = Grid.jqGrid('getCell', selectedRowId, 'Remarks');

            ReturnPartDate = moment(new Date(ReturnPartDate)).format("YYYY-MM-DD");
            ReturnPartIncomingDate = moment(new Date(ReturnPartIncomingDate)).format("YYYY-MM-DD");
            ReturnPartStatus = ReturnPartStatus[1].split('<')
            //ReturnPartIncomingTime = moment(new Date(ReturnPartIncomingTime)).format("H:i");

            $("#crud-ReturnPartNumber").val(id);
            $("#crud-ReturnPartDate").val(ReturnPartDate);
            $("#crud-ReturnPartSupplierId").val(ReturnPartSupplierId);
            $("#crud-ReturnPartIncomingDate").val(ReturnPartIncomingDate);
            $("#crud-ReturnPartIncomingTime").val(ReturnPartIncomingTime);
            $("#crud-ReturnPartRemarks").val(ReturnPartRemarks);
            $("#approvalInfo").text(ReturnPartApproval);

            var showhide=''
            if (ReturnPartStatus[0] === 'Open') {
                showhide = 'showCol';
                canSelect = true;
            } else {
                showhide = 'hideCol';
                canSelect = false;
            }
            $("#jqGridCrudSupplierDetail").jqGrid(showhide, "cb")
            $("#jqGridCrudSupplierDetail").jqGrid(showhide, "Action")
            $("#jqGridCrudSupplierDetail").jqGrid(showhide, "RowStatus")
            $("#jqGridCrudSupplierDetail").jqGrid(showhide, "StockQty")
            $("#jqGridCrudSupplierDetail").jqGrid(showhide, "StockKanban")
            $("#jqGridCrudSupplierDetail").jqGrid(showhide, "OutstandingKanban")
            $("#jqGridCrudSupplierDetail").jqGrid(showhide, "OutstandingQty")

        }

        $(".selectpicker").selectpicker('refresh');

        switch (action) {
            case "Create":

                canSelect = true;
                $("#jqGridCrudSupplierDetail").jqGrid('showCol', "cb")
                $("#jqGridCrudSupplierDetail").jqGrid('showCol', "Action")
                $("#jqGridCrudSupplierDetail").jqGrid('showCol', "RowStatus")
                $("#jqGridCrudSupplierDetail").jqGrid('showCol', "StockQty")
                $("#jqGridCrudSupplierDetail").jqGrid('showCol', "StockKanban")
                $("#jqGridCrudSupplierDetail").jqGrid('showCol', "OutstandingKanban")
                $("#jqGridCrudSupplierDetail").jqGrid('showCol', "OutstandingQty")

                $("#crud-ReturnPartNumber").attr("readonly", true);
                $("#crud-ReturnPartDate").attr("readonly", true);
                //$("#crud-ReturnPartIncomingDate").attr("readonly", true); open start 19-jan-2023 req pak dewi
                //$("#crud-ReturnPartIncomingTime").attr("readonly", true); open s/d 19-mar-2021 req pak ahmad
                $("#crud-ReturnPartReturnUnitQty").attr("readonly", true);
                $("#crud-ReturnPartDate").val(newdate);
                $("#crudReturnPartModal .modal-dialog").removeClass("modal-success").removeClass("modal-danger").removeClass("modal-warning").removeClass("modal-info").addClass("modal-primary")
                $("#crudReturnPartModal .modal-title").html('<span class="fa fa-plus-square"></span> '+ action + ' @ViewBag.Title');
                $('#crudReturnPartModal').modal('show');
                $("#crud-ReturnPartSupplierId").focus();
                break;
            case "Update":
                $("#crud-ReturnPartNumber").attr("readonly", true);
                $("#crud-ReturnPartDate").attr("readonly", true);
                $("#crud-ReturnPartSupplierId").attr("disabled", true);
                $("#crud-ReturnPartIncomingDate").attr("readonly", true);
                $("#crud-ReturnPartIncomingTime").attr("readonly", true);
                $("#crud-ReturnPartReturnUnitQty").attr("readonly", true);
                $("#crudReturnPartModal .modal-dialog").removeClass("modal-primary").removeClass("modal-danger").removeClass("modal-warning").removeClass("modal-info").addClass("modal-success")
                $("#crudReturnPartModal .modal-title").html('<span class="fa fa-pencil-square"></span> '+ action + ' @ViewBag.Title');
                $('#crudReturnPartModal').modal('show');
                $("#crud-ReturnPartRemarks").focus();
                break;
            case "Delete":
                $("#crudReturnPartForm input").each(function () {
                    $(this).attr("disabled",true);
                });
                $("#crud-ReturnPartSupplierId").attr("disabled", true);
                $('#btn-addReturnPartDetail').attr('disabled', true);
                $('#btn-scanReturnPartDetail').attr('disabled', true);
                $("#btn-crudReturnPart").html("<span class='fa fa-trash'></span> Delete");
                $("#crudReturnPartModal .modal-dialog").removeClass("modal-primary").removeClass("modal-success").removeClass("modal-warning").removeClass("modal-info").addClass("modal-danger")
                $("#crudReturnPartModal .modal-title").html('<span class="fa fa-trash"></span> '+ action + ' @ViewBag.Title');
                $('#crudReturnPartModal').modal('show');
                break
            case "Canceled":
                $("#crudReturnPartForm input").each(function () {
                    $(this).attr("disabled",true);
                });
                $("#crud-ReturnPartSupplierId").attr("disabled", true);
                $('#btn-addReturnPartDetail').attr('disabled', true);
                $('#btn-scanReturnPartDetail').attr('disabled', true);
                $("#btn-crudReturnPart").html("<span class='fa fa-ban'></span> Cancel Order");
                $("#crudReturnPartModal .modal-dialog").removeClass("modal-primary").removeClass("modal-success").removeClass("modal-danger").removeClass("modal-info").addClass("modal-warning")
                $("#crudReturnPartModal .modal-title").html('<span class="fa fa-ban"></span> '+ action + ' @ViewBag.Title');
                $('#crudReturnPartModal').modal('show');
                break
            case "Closed":
                $("#crudReturnPartForm input").each(function () {
                    $(this).attr("disabled",true);
                });
                $("#crud-ReturnPartSupplierId").attr("disabled", true);
                $('#btn-addReturnPartDetail').attr('disabled', true);
                $('#btn-scanReturnPartDetail').attr('disabled', true);
                $("#btn-crudReturnPart").html("<span class='fa fa-sign-out'></span> Closing Order");
                $("#crudReturnPartModal .modal-dialog").removeClass("modal-primary").removeClass("modal-success").removeClass("modal-danger").removeClass("modal-warning").addClass("modal-info")
                $("#crudReturnPartModal .modal-title").html('<span class="fa fa-sign-out"></span> '+ action + ' @ViewBag.Title');
                $('#crudReturnPartModal').modal('show');
                break
        }

        reloadGridSupplierDetail();

    }

    $('#crud-ReturnPartSupplierId').change(function () {
        $('#crudReturnPartError').html("");
    });


    $(function () {
        var focusedElement;
        $(document).on('focus', 'input', function () {
            if (focusedElement == this) return; //already focused, return so user can now place cursor at specific point in input.
            focusedElement = this;
            setTimeout(function () { focusedElement.select(); }, 100); //select all text in any field on focus for easy re-entry. Delay sightly to allow focus to "stick" before selecting.
        });
    });

    $(function () {
        $gridSupplierDetail = $("#jqGridCrudSupplierDetail").jqGrid({
            url: "@Url.Action("ReturnPartDetailListJson", "Purchase")",
            mtype: "GET",
            datatype: "json",
            postData: { returnnumber: "*" },
            colModel: [
                { label: 'ACTION', name: 'Action', editable: false, align: 'center', fixed: true, width: 80, sortable: false, formatter: actionSupplierDetailFormatter },
                { label: 'STATUS', name: 'RowStatus', editable: false, align: 'center', fixed: true, width: 60, sortable: false, formatter: statusCrudFormatter },
                { label: 'UNIQUE NUMBER', name: 'UniqueNumber', align: 'center', fixed: true, width: 80, sortable: false },
                { label: 'PART NUMBER', name: 'PartNumber', key: true, align: 'center', fixed: true, width: 160, sortable: false },
                { label: 'PART NAME', name: 'PartName', align: 'left', fixed: true, width: 310, sortable: false },
                { label: 'MODEL', name: 'PartModel', align: 'center', fixed: true, width: 80, sortable: false },
                { label: 'UNIT', name: 'UnitLevel2', align: 'center', fixed: true, width: 80, sortable: false },
                { label: 'TYPE PACKING', name: 'PackingId', align: 'center', fixed: true, width: 80, sortable: false },
                { label: 'NG STOCK QTY', name: 'StockQty', align: 'right', fixed: true, width: 80, formatter: 'number', sortable: false },
                { label: 'RETURN QTY', name: 'ReturnUnitQty', align: 'right', fixed: true, width: 80, formatter: 'number', sortable: false },
            ],
            gridview: true,
            //pager: '#jqGridPagerCrudSupplierDetail',
            loadonce: true,
            height: 220,
            pgbuttons: false,
            pgtext: null,
            viewrecords: true,
            rowNum: 9999,
            rownumbers: true,
            rownumWidth: 30,
            autoresizeOnLoad: true,
            autowidth: true,
            multiselect: true,
            shrinkToFit: true,
            fromServer: true,
            loadComplete: function () {
                var $this = $(this), ids = $this.jqGrid('getDataIDs'), i, l = ids.length;
                var action = $("#ReturnPartAction").val();
                for (i = 0; i < l; i++) {
                    $this.jqGrid('editRow', ids[i], true);
                    if (action === "Create") {
                        parameters =
                        {
                            RowStatus: action,
                        }
                        $("#jqGridCrudSupplierDetail").jqGrid('setRowData', ids[i], parameters);
                    }
                }
            },
            ondblClickRow: function (rowid) {
                crudReturnPartDetail("Update", rowid);
            },
        });
        $('#jqGridCrudSupplierDetail').jqGrid('navGrid', '#jqGridPagerCrudSupplierDetail',
            { search: false, edit: false, add: false, del: false},
        );

    });

    function reloadGridSupplierDetail() {

        var formaction  = $("#ReturnPartAction").val();
        var supplierid  = $("#crud-ReturnPartSupplierId").val();
        var returnnumber = $("#crud-ReturnPartNumber").val();

        $("#jqGridCrudSupplierDetail").jqGrid('setGridParam', {
            datatype: 'json',
            mtype: 'GET',
            postData: {
                returnnumber: returnnumber,
                supplierid: supplierid,
                formAction: formaction,
            }
        }).trigger('reloadGrid');

    };

    function actionSupplierDetailFormatter(cellvalue, options, rowObject) {
        var rowid = rowObject.PartNumber;
        var formaction = $("#ReturnPartAction").val();
        var canupdate = '@ViewBag.canUpdate';
        var candelete = '@ViewBag.canDelete';

        if (formaction === "Closed" || formaction === "Canceled" || formaction === "Delete") {
            canupdate = "disabled";
            candelete = "disabled";
        }

        var btn = "<div class='table-link'>";
        btn += "<a href='#' id='btn-update" + rowid + "' class='btn btn-sm btn-primary text-white " + canupdate + "' onclick=\"crudReturnPartDetail('Update','" + rowid + "')\" datatoogle='tooltip' title='Edit @ViewBag.Title Item [ " + rowObject.PartNumber + " ]'>";
        btn += "<span class='fa fa-pencil'></span>";
        btn += "</a> ";
        btn += "<a href='#' id='btn-delete" + rowid + "' class='btn btn-sm btn-danger text-white " + candelete + "' onclick=\"crudReturnPartDetail('Delete','" + rowid + "')\" datatoogle='tooltip' title='Delete @ViewBag.Title Item [ " + rowObject.PartueNumber + " ]'>";
        btn += "<span class='fa fa-trash-o'></span>";
        btn += "</a></div>";
        return btn;
    }


    function statusCrudFormatter(cellvalue, options, rowObject) {
        var action = $("#ReturnPartAction").val();
        switch (cellvalue) {
            case "Create":
                return "<span class='badge badge-primary'>New</span>"
                break;
            case "Update":
                return "<span class='badge badge-success'>Updated</span>"
                break;
            case "Delete":
                return "<span class='badge badge-danger'>Removed</span>"
                break;
            default:
                //if (action === "Create") {
                //    return "<span class='badge badge-primary'>New</span>"
                //} else {
                //    return "";
                //}
                return "";
                break;
        }
    }

    function crudReturnPartDetailMove(action) {
        var $grid = $("#jqGridCrudSupplierDetail");
        var id = $("#crud-ReturnPartDetailPartNumber").val();
        var rowIndex = $grid.jqGrid('getInd', id);
        var dataIDs = $grid.getDataIDs();
        var datalen = (dataIDs.length) - 1;

        $("#btn-prev").removeClass("disabled");
        $("#btn-next").removeClass("disabled");


        switch (action) {
            case "prev":

                rowIndex = parseInt(rowIndex) - 2;

                if (dataIDs.length >= rowIndex && rowIndex >= 0) {

                    var prevID = dataIDs[rowIndex];
                    crudReturnPartDetail("Update", prevID)
                    $grid.setSelection(prevID, false);

                }

                if (rowIndex <= 0) {
                    $("#btn-prev").addClass("disabled");
                }

                break;

            case "next":

                if (dataIDs.length > rowIndex) {

                    var nextID = dataIDs[rowIndex];
                    crudReturnPartDetail("Update", nextID)
                    $grid.setSelection(nextID, false);

                }

                if (datalen <= rowIndex) {
                    $("#btn-next").addClass("disabled");
                }

                break;

        }

    }

    function crudReturnPartDetail(action, id) {

        document.getElementById("crudReturnPartDetailForm").reset();
        $('#crudReturnPartDetailForm').removeClass('was-validated');
        $('#crudReturnPartError').html("");
        $('#crudReturnPartDetailError').html("");
        $("#btn-crudReturnPartDetail").html("<span class='fa fa-dot-circle-o'></span> Submit");
        $("#btn-prev").addClass("disabled");
        $("#btn-next").addClass("disabled");
        $("#btn-searchPart").removeClass("disabled");

        $("#crudReturnPartDetailForm :input").each(function () {
            $(this).removeAttr("readonly").removeAttr("disabled").removeClass('error').next('label.error').remove().val("");
        });

        $("#ReturnPartDetailAction").val(action);
        var supplierid = $("#crud-ReturnPartSupplierId").val();

        if (supplierid === "") {
            var errMsg = '<div class="alert alert-warning mt-2" role="alert"><small class="text-danger"><b>Not completed fields!</b><br/>Please type Supplier completely before add @ViewBag.Title Detail.</small></div>'
            $('#crudReturnPartError').html(errMsg);
            return false;
        }

        if (id != "") {
            var $grid = $("#jqGridCrudSupplierDetail");
            var rowData = $grid.jqGrid("getRowData", id),
                RowStatus = rowData.RowStatus.split(">"),
                UniqueNumber = rowData.UniqueNumber,
                PartNumber = rowData.PartNumber,
                PartName = rowData.PartName,
                Model = rowData.PartModel,
                QtyByKanban = rowData.UnitQty,
                Unit = rowData.UnitLevel2,
                PackingId = rowData.PackingId,
                StockQty = rowData.StockQty,
                StockKanban = rowData.StockKanban,
                ReturnUnitQty = rowData.ReturnUnitQty;

            if (RowStatus != "") {
                RowStatus = RowStatus[1].split("<");
                RowStatus = RowStatus[0];
            }

            var OnOrder = GetOnOrderStock($("#crud-ReturnPartNumber").val(), $("#crud-ReturnPartSupplierId").val(), PartNumber);
            if (OnOrder === null || OnOrder === "") {
                OnOrder = 0;
            }
            var MaxOrder = parseInt(StockQty) - parseInt(StockKanban) - parseInt(OnOrder);
            var MinOrder = 1;
            if (parseInt(MaxOrder) < 1) {
                MinOrder = 1;
                MaxOrder = 0;
            }

            $("#ReturnPartDetailRowStatus").val(RowStatus);
            $("#crud-ReturnPartDetailUniqueNumber").val(UniqueNumber);
