            $("#crud-ReturnPartDetailPartModel").val(Model);
            $("#crud-ReturnPartDetailUnitQty").val(QtyByKanban);
            $("#crud-ReturnPartDetailUnit").val(Unit);
            $("#crud-ReturnPartDetailPackingId").val(PackingId);
            $("#crud-ReturnPartDetailStockQty").val(StockQty);
            $("#crud-ReturnPartDetailReturnUnitQty").val(ReturnUnitQty);

        }

        switch (action) {
            case "Create":
                $("#crud-ReturnPartDetailUniqueNumber").attr("disabled", true);
                $("#crud-ReturnPartDetailPartNumber").attr("disabled", true);
                $("#crud-ReturnPartDetailPartName").attr("disabled", true);
                $("#crud-ReturnPartDetailPartModel").attr("disabled", true);
                $("#crud-ReturnPartDetailUnitQty").attr("disabled", true);
                $("#crud-ReturnPartDetailUnit").attr("disabled", true);
                $("#crud-ReturnPartDetailPackingId").attr("disabled", true);
                $("#crud-ReturnPartDetailStockQty").attr("disabled", true);
                $("#crudReturnPartDetailModal .modal-dialog").removeClass("modal-success").removeClass("modal-danger").addClass("modal-primary")
                $("#crudReturnPartDetailModal .modal-title").html('<span class="fa fa-plus-square"></span> ' + action + ' @ViewBag.Title Item');
                $('#crudReturnPartDetailModal').modal('show');
                $("#btn-searchPart").focus();
                break;
            case "Update":
                $("#crud-ReturnPartDetailUniqueNumber").attr("disabled", true);
                $("#crud-ReturnPartDetailPartNumber").attr("disabled", true);
                $("#crud-ReturnPartDetailPartName").attr("disabled", true);
                $("#crud-ReturnPartDetailPartModel").attr("disabled", true);
                $("#crud-ReturnPartDetailUnitQty").attr("disabled", true);
                $("#crud-ReturnPartDetailUnit").attr("disabled", true);
                $("#crud-ReturnPartDetailPackingId").attr("disabled", true);
                $("#crud-ReturnPartDetailStockQty").attr("disabled", true);
                $("#crudReturnPartDetailModal .modal-dialog").removeClass("modal-primary").removeClass("modal-danger").addClass("modal-success")
                $("#crudReturnPartDetailModal .modal-title").html('<span class="fa fa-pencil-square"></span> ' + action + ' @ViewBag.Title Item');
                $('#crudReturnPartDetailModal').modal('show');
                $("#crud-ReturnPartDetailReturnUnitQty").focus().select();

                var rowIndex = $grid.jqGrid('getInd', PartNumber);
                var dataIDs = $grid.getDataIDs();
                var datalen = (dataIDs.length);

                $("#btn-prev").removeClass("disabled");
                $("#btn-next").removeClass("disabled");

                if (rowIndex === 1) {
                    $("#btn-prev").addClass("disabled");
                }
                if (rowIndex === datalen) {
                    $("#btn-next").addClass("disabled");
                }
                break;
            case "Delete":
                $("#crudReturnPartDetailForm input").each(function () {
                    $(this).attr("disabled", true);
                });
                $("#btn-searchPart").addClass("disabled");
                $("#btn-crudReturnPartDetail").html("<span class='fa fa-trash'></span> Delete");
                $("#crudReturnPartDetailModal .modal-dialog").removeClass("modal-primary").removeClass("modal-success").addClass("modal-danger")
                $("#crudReturnPartDetailModal .modal-title").html('<span class="fa fa-trash"></span> ' + action + ' @ViewBag.Title Item');
                $('#crudReturnPartDetailModal').modal('show');
                break
        }
    }

