<div class="modal animated fadeIn" id="crudReturnPartModal" tabindex="-1" role="dialog" aria-hidden="true" data-keyboard="false" data-backdrop="false">
    <div class="modal-dialog modal-xl modal-dialog-centered modal-primary" role="document" style="width:90% !important">
        <div class="modal-content">
            <form id="crudReturnPartForm" class="needs-validation" novalidate>
                <div class="modal-header">
                    <h6 class="modal-title"><span class="fa fa-plus-square"></span> @ViewBag.Title</h6>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <div class="modal-body">
                    <div class="row">
                        <div class="col-md-3">
                            <div class="form-group form-group-sm p-0 m-1 row">
                                <label for="crud-ReturnPartNumber" class="col-sm-3 col-form-label col-form-label-sm">Number</label>
                                <div class="col-sm-9">
                                    <input type="text" class="form-control form-control-sm bg-white" placeholder="Auto Generate" id="crud-ReturnPartNumber">
                                </div>
                            </div>
                            <div class="form-group form-group-sm p-0 m-1 row">
                                <label for="crud-ReturnPartDate" class="col-sm-3 col-form-label col-form-label-sm">Date</label>
                                <div class="col-sm-9">
                                    <input type="date" class="form-control form-control-sm bg-white datepicker" id="crud-ReturnPartDate" required>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-5">
                            <div class="form-group form-group-sm p-0 m-1 row">
                                <label for="crud-ReturnPartSupplierId" class="col-sm-3 col-form-label col-form-label-sm">Supplier</label>
                                <div class="col-sm-9">
                                    <select class="form-control form-control-sm selectpicker" data-live-search="true" data-size="8" id="crud-ReturnPartSupplierId" name="SupplierId" required></select>
                                </div>
                            </div>
                            <div class="form-group form-group-sm p-0 m-1 row">
                                <label for="crud-ReturnPartIncomingDate" class="col-sm-3 col-form-label col-form-label-sm">Incoming</label>
                                <div class="col-sm-9">
                                    <div class="input-group input-group-sm">
                                        <div class="input-group-prepend">
                                            <span class="input-group-text"><i class="fa fa-calendar"></i></span>
                                        </div>
                                        <input type="date" class="form-control form-control-sm bg-white border-right-0 datepicker" style="width:70px" id="crud-ReturnPartIncomingDate" name="IncomingDate" required>
                                        <div class="input-group-prepend">
                                            <span class="input-group-text"><i class="fa fa-clock-o"></i></span>
                                        </div>
                                        <input type="time" class="form-control form-control-sm bg-white" id="crud-ReturnPartIncomingTime" name="IncomingTime" required>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-4">
                            <div class="form-group form-group-sm p-0 m-1 row">
                                <label for="crud-ReturnPartRemarks" class="col-sm-4 col-form-label col-form-label-sm">Remarks</label>
                                <div class="col-sm-8">
                                    <textarea rows="2" class="form-control form-control-sm bg-white" id="crud-ReturnPartRemarks" name="Remarks"></textarea>
                                </div>
                            </div>
                            

                        </div>
                        <div class="col-md-12 pt-2">
                            <table id="jqGridCrudSupplierDetail"></table>
                            <div id="jqGridPagerCrudSupplierDetail"></div>
                        </div>
                    </div>

                    <div id="crudReturnPartError"></div>
                    <input type="hidden" id="ReturnPartAction" />

                </div>
                <div class="modal-footer">

                    <div class="ml-3" style="position:absolute; left:0 !important">
                        <div class="row">
                            <div class="col">
                                <button id="btn-scanReturnPartDetail" type="button" class="btn btn-sm btn-outline-dark" onclick="crudReturnPartDetail('Scan','')" hidden><span class="fa fa-qrcode"></span> Scan QRCode</button>
                                <button id="btn-addReturnPartDetail" type="button" class="btn btn-sm btn-outline-dark" style="margin-top: 0 !important" onclick="crudReturnPartDetail('Create','')"><span class="fa fa-plus"></span> New Part Item</button>
                            </div>
                        </div>
                    </div>

                    <button id="btn-crudReturnPart" type="submit" class="btn btn-sm btn-primary btn-spin" dismiss="modal"><span class="fa fa-dot-circle-o"></span> Submit</button>
                    <button type="button" class="btn btn-sm btn-danger" data-dismiss="modal"><span class="fa fa-times"></span> Cancel</button>
                </div>
            </form>

        </div>
    </div>
</div>

<div id="contextMenu" class="dropdown clearfix">
